@model GrupoSelect.Web.ViewModel.ComponentsVM

@{
    ViewData["Title"] = "Dashboard - Financeiro";
}

@*<div class="title_left">
    <h3>Form advanced</h3>
</div>*@

<div class="clearfix"></div>
<div class="row">
@foreach (var item in Model.LstTile)
{
    @Html.Partial("Components/_Tile", item)
}
</div>
<script src="~/js/chart/chart.min.js"></script>
<div class="row">
@foreach (var item in Model.LstChartMoney)
{
    <div class="col-md-6 col-sm-6 col-xs-12">
        @Html.Partial("Components/_ChartMoney", item)
    </div>
}
</div>